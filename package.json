  },
  "dependencies": {
    "base-x": "^4.0.0",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.0",
    "envalid": "^7.2.2",
    "express": "^4.17.3",
    "express-prom-bundle": "^6.4.1",
    "prom-client": "^14.0.1"
